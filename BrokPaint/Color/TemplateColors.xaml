<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    >
    <!--Colors Button-->
    <SolidColorBrush x:Key="bgButton" Color="#09B8B8"/>
    <SolidColorBrush x:Key="bbButton" Color="#009999"/>
    <SolidColorBrush x:Key="fgButton" Color="#CCFFFF"/>

    <!--Colors TextBox-->
    <SolidColorBrush x:Key="bgTextBoxFalse" Color="#2D2533"/>
